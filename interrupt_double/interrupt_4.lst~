    24: .MACRO CFG_HANDLER
    25:      LDI R16,(1<<ISC10)
    26:      sts EICRA,R16
    27:      sbi EIMSK,INT1
    28:      ;
    29:      LDS R16,PCICR
    30:      ORI R16,(1<<PCIE0)
    31:      STS PCICR,R16
    32:      ; Determina qual pino estar치 escutando
    33:      LDS R17,PCMSK0
    34:      ORI R17,(1<<PCINT3)
    35:      STS PCMSK0,R17
    36: .ENDMACRO
